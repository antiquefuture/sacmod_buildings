<p id="notice"><%= notice %></p>

<h1>Buildings</h1>

<table>
  <thead>
    <tr>
      <th>Architect</th>
      <th>Apn</th>
      <th>Year</th>
      <th>Address1</th>
      <th>City</th>
      <th>Zip</th>
      <th>Family</th>
      <th>Type</th>
      <th>Status</th>
      <th>Source</th>
      <th>Notes</th>
      <th>Gsv</th>
      <th>Key</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @buildings.each do |building| %>
      <tr>
        <td><%= building.architect_id %></td>
        <td><%= building.apn %></td>
        <td><%= building.year %></td>
        <td><%= building.address1 %></td>
        <td><%= building.city %></td>
        <td><%= building.zip %></td>
        <td><%= building.family %></td>
        <td><%= building.type %></td>
        <td><%= building.status %></td>
        <td><%= building.source %></td>
        <td><%= building.notes %></td>
        <td><%= building.gsv %></td>
        <td><%= building.key %></td>
        <td><%= link_to 'Show', building %></td>
        <td><%= link_to 'Edit', edit_building_path(building) %></td>
        <td><%= link_to 'Destroy', building, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Building', new_building_path %>
